CPP INCLUDE <stdio.h>
DECLARE FUNCTION main TYPE int
BEGIN
DECLARE VARIABLE i TYPE int 
LOOP FIRST VARIABLE i GETS LITERAL INTEGER 1 END LITERAL
CONDITION VARIABLE i LESSTHAN LITERAL INTEGER 101 END LITERAL
CYCLE PREINCREMENT VARIABLE i
BEGIN

IF VARIABLE i MODULO LITERAL INTEGER 3 END LITERAL EQUALS LITERAL INTEGER 0 END LITERAL AND VARIABLE i MODULO LITERAL INTEGER 5 END LITERAL EQUALS LITERAL INTEGER 0 END LITERAL
BEGIN
        CALL printf ARG LITERAL STRING "FizzBuzz" END LITERAL
END SCOPE

ELSE IF VARIABLE i MODULO LITERAL INTEGER 3 END LITERAL EQUALS LITERAL INTEGER 0 END LITERAL
BEGIN
        CALL printf ARG LITERAL STRING "Fizz" END LITERAL
END SCOPE

ELSE IF VARIABLE i MODULO LITERAL INTEGER 5 END LITERAL EQUALS LITERAL INTEGER 0 END LITERAL
BEGIN
        CALL printf ARG LITERAL STRING "Buzz" END LITERAL
END SCOPE

CALL printf ARG LITERAL STRING "\n" END LITERAL

END SCOPE

END SCOPE

END